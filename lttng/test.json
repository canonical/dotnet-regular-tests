{
  "name": "lttng",
  "enabled": true,
  "requiresSdk": true,
  "version": "2.1",
  "versionSpecific": false,
  "type": "bash",
  "cleanup": true,
  "skipWhen": [
    "runtime=mono", // mono does not use lttng
  ],
  "ignoredRIDs":[
    "alpine", // lttng <=2,13,0 is broken
    "centos",
    "fedora", // see https://github.com/redhat-developer/dotnet-regular-tests/issues/202
    "rhel"
  ]
}
