{
  "name": "bundled-libunwind",
  "enabled": true,
  "requiresSdk": false,
  "version": "2.0",
  "versionSpecific": false,
  "type": "bash",
  "cleanup": true,
  "skipWhen": [
    "runtime=mono", // mono does not use libunwind
    // see https://github.com/canonical/dotnet-regular-tests/issues/7
    "os=ubuntu,version=6",
    "os=ubuntu,version=9,arch=x64",
    "os=ubuntu,version=10,arch=x64",
  ],
  "ignoredRIDs":[
    "alpine",
    "linux-arm",
  ]
}
